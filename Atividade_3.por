programa {
  funcao inicio() {
    inteiro x, y, soma, mult

    escreva("Digite o primeiro número:")
    leia(x)

    escreva("Digite o segundo número:")
    leia(y)

    soma = x + y
    escreva("O resultado da soma é :", soma,"\n")

    mult = x * y
    escreva ("O resultado da multiplicação é:", mult)
  }
}
